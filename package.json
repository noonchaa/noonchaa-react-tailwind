{
  "name": "noonchaa-react-tailwind",
  "version": "1.0.1",
  "description": "React component library with tailwind css",
  "author": "noonchaa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/noonchaa/noonchaa-react-tailwind.git"
  },
  "homepage": "https://noonchaa.github.io/noonchaa-react-tailwind",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "contributors": [
    "noonchaa"
  ],
  "keywords": [
    "react",
    "tailwind",
    "noonchaa"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c --watch",
    "prepublishOnly": "npm run build",
    "predeploy": "npm run build-sb",
    "deploy": "gh-pages -d storybook-static",
    "test": "jest --verbose --watch --coverage",
    "sb": "start-storybook -p 6006",
    "build-sb": "build-storybook"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/@testing-library/jest-dom",
      "<rootDir>/node_modules/@testing-library/react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.7",
    "postcss": "^8.4.6",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.0.18"
  }
}
